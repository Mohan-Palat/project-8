<h1>Here are the boxes for <%=user.userName%></h1>

<div><a href="/users/<%=user._id%>/boxes/new">Create a New Box</a></div>

<% user.boxes.forEach(box => { %>
<div class="row">
  <div class="col s12 m4">
    <div class="card blue-grey darken-1">
      <div class="card-content white-text">
        <span class="card-title">Box <%=box.identNum%> </span>
        <h6>Contents: <%=box.category%></h4>
        <p><%=box.desc%></p>
      </div>
      <div class="card-action">
        <a href="/users/<%=user._id%>/boxes/<%=box._id%>">Details</a>
      </div>
    </div>
  </div>
</div>
<% }) %>